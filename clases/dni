<?php
    $dni="00000046";
    $dni2=intval($dni, 10);
    $aux=$dni2%23;
    switch ($aux){
        case(0):
            echo "Su letra es: T, asi que su DNI es: $dni T";break;
        case(1):
            echo "Su letra es: R, asi que su DNI es: $dni R";break;
